{
  "name": "ContestOrganizer",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Ionic App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prod-clean": "rm -rf /var/www/marius.click/html/contest/*",
    "prod-push": "cp -r www/* /var/www/marius.click/html/contest/",
    "setSrcBasePath": "sed -i 's/src=\"/src=\"\\/contest/g' www/index.html",
    "setHrefBasePath": "sed -i 's/href=\"/href=\"\\/contest/g' www/index.html",
    "prod": "npm run prod-clean && npm run setHrefBasePath && npm run setSrcBasePath && npm run prod-push",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@ionic/core": "^6.7.0",
    "@stencil/core": "^3.2.0",
    "@types/jest": "^29.5.0",
    "ionicons": "^7.1.0",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "puppeteer": "^19.8.0"
  },
  "license": "MIT"
}
